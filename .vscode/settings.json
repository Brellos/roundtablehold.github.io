{
    "emeraldwalk.runonsave": {
        "autoClearConsole": true,
        "commands": [
            {
                "match": ".*",
                "cmd": "python generate.py"
            }
        ]
    },
    "liveServer.settings.ignoreFiles": [
        ".vscode/**",
        "**/*.scss",
        "**/*.sass",
        "**/*.ts",
        "**/*.py",
        "**/*.yaml",
        "**/*.js",
        "**/*.css"
    ],
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.root": "/docs",
    "editor.wordWrap": "on",
    "editor.lineHeight": 1.2
}